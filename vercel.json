{
  "version": 2,
  "functions": {
    "api/simple_api.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/simple_api.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "MISTRAL_API_KEY": "${MISTRAL_API_KEY}"
  }
} 